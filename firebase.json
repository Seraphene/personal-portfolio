{
  "hosting": {
    "public": "out",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/blockchain",
        "destination": "https://ltoblockchain.duckdns.org/",
        "type": 301
      },
      {
        "source": "/lost-and-found",
        "destination": "https://ublf-2.onrender.com",
        "type": 301
      },
      {
        "source": "/prizmak-media",
        "destination": "/prizmak",
        "type": 301
      }
    ]
  }
}